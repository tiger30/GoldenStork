<?xml version="1.0" encoding="utf-8"?>
<com.android.gs.components.slidemenu.MainLayout xmlns:android="http://schemas.android.com/apk/res/android"
     xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mLayout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

    <!-- menu -->

    <com.android.gs.components.slidemenu.SlideMenu
        android:id="@+id/mnSlideMenu"
        android:layout_width="fill_parent"
        android:layout_height="match_parent" >
    </com.android.gs.components.slidemenu.SlideMenu>

    <!-- end menu -->


    <!-- This is where fragment will show up -->

    <FrameLayout
        android:id="@+id/activity_main_content_fragment"
        android:layout_width="fill_parent"
        android:layout_height="match_parent" >
    </FrameLayout>

</com.android.gs.components.slidemenu.MainLayout>